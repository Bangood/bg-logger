{
  "name": "bg-logger",
  "version": "1.0.3",
  "description": "logger",
  "main": "dist/index.js",
  "scripts": {
    "test:watch": "npm t -- -w",
    "test": "mocha  src/**/*.test.js --require babel-register",
    "cover": "nyc mocha src/**/*.test.js --require babel-register",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "commit": "npx git-cz",
    "semantic-release": "semantic-release",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bangood/bg-logger.git"
  },
  "keywords": [
    "logger"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "'bangood' <'2419281635@qq.com'>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bangood/bg-logger/issues"
  },
  "homepage": "https://github.com/Bangood/bg-logger#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "chai": "4.2.0",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.4",
    "istanbul": "0.4.5",
    "mocha": "6.0.2",
    "nyc": "13.3.0",
    "rimraf": "2.6.3",
    "semantic-release": "^15.13.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "dependencies": {
    "winston": "3.2.1",
    "winston-daily-rotate-file": "3.8.0",
    "signale": "1.4.0"
  }
}
